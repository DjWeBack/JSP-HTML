<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jobMapper">
 <select id="getJobList" resultType="java.util.Map" parameterType="map">
 	select j_name from job
 
 	 where j_yn = #{j_yn}

 	  	
 </select>
</mapper>